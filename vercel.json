{
  "buildCommand": "pip install -r requirements.txt && python portfolioLunah/manage.py collectstatic --noinput",
  "builds": [
    {
      "src": "portfolioLunah/portfolioLunah/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles_build/static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "staticfiles_build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "portfolioLunah/portfolioLunah/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "portfolioLunah.settings",
    "PYTHONPATH": "./:./portfolioLunah"
  }
}
