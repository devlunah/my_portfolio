{
  "builds": [
    {
      "src": "portfolioLunah/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "portfolioLunah/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "portfolioLunah.settings",
    "PYTHONPATH": "./portfolioLunah"
  }
}
